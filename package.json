{
    "name": "nextjs-compose",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "prisma generate && next build",
        "start": "next start",
        "lint": "next lint",
        "db:browser": "yarn prisma studio",
        "db:migrate": "yarn prisma migrate dev --name init --create-only && yarn prisma generate",
        "db:push": "yarn prisma db push"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.15",
        "@next-auth/prisma-adapter": "^1.0.7",
        "@prisma/client": "^5.7.0",
        "bcryptjs": "^2.4.3",
        "fetch-ponyfill": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "next": "14.0.4",
        "next-auth": "^4.24.5",
        "react": "^18",
        "react-dom": "^18",
        "react-toastify": "^9.1.3",
        "sharp": "^0.32.6",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10.0.1",
        "eslint": "^8",
        "eslint-config-next": "14.0.4",
        "postcss": "^8",
        "prisma": "^5.7.0",
        "tailwind-merge": "^1.14.0",
        "tailwindcss": "^3.3.0",
        "ts-node": "^10.9.2",
        "typescript": "^5"
    },
    "packageManager": "yarn@4.0.2"
}
